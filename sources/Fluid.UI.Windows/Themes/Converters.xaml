                    xmlns:hertz="clr-namespace:Fluid.UI.Windows.Converters.UnitConverters.Hertz"
                    xmlns:channels="clr-namespace:Fluid.UI.Windows.Converters.UnitConverters.Channels"
                    xmlns:voltage="clr-namespace:Fluid.UI.Windows.Converters.UnitConverters.Voltage">

    <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
    <converters:ColorToSolidColorBrushConverter x:Key="ColorToSolidColorBrushConverter"/>
    <converters:CountToVisibilityConverter x:Key="CountToVisibilityConverter"/>
    <converters:FileNameToImageBrushConverter x:Key="FileNameToImageBrushConverter"/>
